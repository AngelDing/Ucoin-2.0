<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
        <title>@ViewBag.Title</title>
        <link rel="stylesheet" type="text/css" href="~/Content/style.css" media="all">
        <script src="//ajax.aspnetcdn.com/ajax/jQuery/jquery-1.7.2.min.js" type="text/javascript"></script>
        <script type="text/javascript" src="~/Content/form/form.js"></script>
        <script type="text/javascript" src="~/Content/tabs/tabs.js"></script>
	
	    <link type="text/css" href="~/Content/form/ui/css/custom-theme/jquery-ui-1.8.19.custom.css" rel="stylesheet" />
        <script src="//ajax.aspnetcdn.com/ajax/jquery.ui/1.8.19/jquery-ui.min.js" type="text/javascript"></script>

        @RenderSection("Head", required: false)
    </head>
<body class="page page_green @(ViewBag.PageType ?? "page_create-conf")">
<nav>
    <div class="page__wrapper quick">
        <ul class="quick__list">
            <li class="quick__item quick__item_bold"><a href="~/">Contoso Conference Management System</a></li>
        </ul>
        @RenderSection("Navigation", required: false)
    </div>
</nav>
    @RenderSection("PageNavigation", required: false) 

    @RenderBody()
<script src="//ajax.microsoft.com/ajax/jQuery.Validate/1.9/jQuery.Validate.min.js" type="text/javascript"></script>
<script src="//ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
@RenderSection("Scripts", required: false)
</body>
</html>